{
  "manifest_version": 2,
  "name": "Sprung for Firefox",
  "version": "1.0",
  "description": "Sprung for Firefox",
  "icons": {
    "48": "icons/spring.png"
  },
  "permissions": [
    "tabs",
    "http://localhost:1234/firefox"
  ],
  "background": {
    "scripts": ["background.js", "jquery-3.2.1.min.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/spring.png",
      "32": "icons/spring.png"
    },
    "browser_style": true,
    "default_popup": "popup.html"
  }
}
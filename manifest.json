{
  "manifest_version": 2,
  "name": "Sprung for Firefox",
  "version": "1.0",
  "description": "Sprung for Firefox",
  "icons": {
    "48": "icons/spring.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["sprung.js"]
    }
  ],
  "permissions": [
    "tabs",
    "http://localhost:1234/firefox"
  ],
  "background": {
    "scripts": ["background.js", "jquery-3.2.1.min.js"]
  }
}
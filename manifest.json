{
  "manifest_version": 2,
  "name": "Sprung for Firefox",
  "version": "1.0",
  "description": "Sprung for Firefox",
  "icons": {
    "48": "icons/spring.png"
  },
  "permissions": [
    "tabs",
    "http://localhost:1234/firefox",
    "http://localhost:1234/test"
  ],
  "background": {
    "scripts": [
      "config.js",
      "jquery-3.2.1.min.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/spring.png",
      "32": "icons/spring.png"
    },
    "browser_style": false,
    "default_popup": "popup.html"
  }
}